    <title>4个方法，2个软件，10个网站下载百度文库</title>
    <link href="http://example.com/2021/05/07/4%E4%B8%AA%E6%96%B9%E6%B3%95%EF%BC%8C2%E4%B8%AA%E8%BD%AF%E4%BB%B6%EF%BC%8C10%E4%B8%AA%E7%BD%91%E7%AB%99%EF%BC%8C%E5%A4%8D%E5%88%B6%E5%92%8C%E4%B8%8B%E8%BD%BD%E7%99%BE%E5%BA%A6%E6%96%87%E5%BA%93/"/>
    <id>http://example.com/2021/05/07/4%E4%B8%AA%E6%96%B9%E6%B3%95%EF%BC%8C2%E4%B8%AA%E8%BD%AF%E4%BB%B6%EF%BC%8C10%E4%B8%AA%E7%BD%91%E7%AB%99%EF%BC%8C%E5%A4%8D%E5%88%B6%E5%92%8C%E4%B8%8B%E8%BD%BD%E7%99%BE%E5%BA%A6%E6%96%87%E5%BA%93/</id>
    <published>2021-05-07T09:17:57.480Z</published>
